<?xml version="1.0" encoding="iso-8859-1"?>

<Project>itlab-vision</Project>

<Description>

    <LineLength>80</LineLength>

    <NameOfClass>
        <name>[NameOfClass]</name>
        <prefix>*</prefix>
    </NameOfClass>
    <DeclarationOrder>
        <public>0</public>
        <protected>1</protected>
        <private>2</private>
    </DeclarationOrder>
    <InternalVariables>
        <regex>[a-z]</regex>
    </InternalVariables>
    <MemberFunctions>
        <regex>[a-z]</regex>
        <length>100</length>
    </MemberFunctions>

    <SemicolonSpace>0</SemicolonSpace>
    <Indent>
        <type>SPACE</type>
        <size>4</size>
        <noHeader>true</noHeader>
        <allowBlockLine>true</allowBlockLine>
    </Indent>
    <Tabs>1</Tabs>
    <EmptyLines>2</EmptyLines>
    <EndOfFileNewLine>1</EndOfFileNewLine>
    
    <Operator>
        <spaceBefore>1</spaceBefore>
        <spaceAfter>1</spaceAfter>
    </Operator>

    <StatementPerLine>
        <maxNumber>1</maxNumber>
        <checkInline>0</checkInline>
    </StatementPerLine>
    <VariablePerLine>
        <maxNumber>1</maxNumber>
    </VariablePerLine>
    <Functions>
        <regex>[a-z]</regex>
        <length>100</length>
    </Functions>

    <BadCharacters>true</BadCharacters>

</Description>
